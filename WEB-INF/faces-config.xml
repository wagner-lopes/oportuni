 <navigation-rule>
  <from-view-id>/apresentacao/principal.jsp</from-view-id>
  <navigation-case>
   <from-outcome>loginAlunoSucesso</from-outcome>
   <to-view-id>/aluno/principal.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>loginResponsavelSucesso</from-outcome>
   <to-view-id>/responsavel/principal.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/apresentacao/principal.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>erro</from-outcome>
   <to-view-id>/erro.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/erro.jsp</from-view-id>
  <navigation-case>
   <from-outcome>principal</from-outcome>
   <to-view-id>/apresentacao/principal.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/aluno/principal.jsp</from-view-id>
  <navigation-case>
   <from-outcome>sair</from-outcome>
   <to-view-id>/index.jsp</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/responsavel/principal.jsp</from-view-id>
  <navigation-case>
   <from-outcome>sair</from-outcome>
   <to-view-id>/index.jsp</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <validator>
  <validator-id>ValidadorBean</validator-id>
  <validator-class>br.ufmg.oportuni.controle.ValidadorBean</validator-class>
 </validator>
 <application>
  <!-- Aplicação do Listener -->
  <action-listener>br.ufmg.oportuni.util.ExceptionHandler</action-listener>
 </application>
 <lifecycle>
  <phase-listener>br.ufmg.oportuni.controle.ListenerFasesJSF</phase-listener>
 </lifecycle>
</faces-config>
